<%= form_for [@shelter, @pet] do |f| %>
  <%= f.label :image %>
  <%= f.text_field :image %><br>

  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :approximate_age %>
  <%= f.text_field :approximate_age %><br>

  <%= f.label :sex %>
  <%= f.text_field :sex %><br>

  <%= f.label :description %>
  <%= f.text_field :description %><br>
  
  <%= f.submit %><br>
<% end %>