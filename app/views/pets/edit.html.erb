<%= form_for @pet do |f| %>

  <%= f.label :image %>
  <%= f.text_field :image %><br>

  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :approximate_age %>
  <%= f.text_field :approximate_age %><br>

  <%= f.label :sex %>
  <%= f.number_field :sex %><br>
  
  <%= f.submit %><br>
  <% end %>